<style>
body {
  background-color: black;
  margin: 0;
  padding: 0;
}
.center {
  position:absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%)
}
.center span {
  color:white;
  font-family:sans-serif;
  animation:animate 0.6s ease infinite alternate;
}
.center span:nth-child(1) {
  animation-delay:0.1s;
}
.center span:nth-child(2) {
  animation-delay:0.2s;
}
.center span:nth-child(3) {
  animation-delay:0.3s;
}
.center span:nth-child(4) {
  animation-delay:0.4s;
}
.center span:nth-child(5) {
  animation-delay:0.5s;
}
.center span:nth-child(6) {
  animation-delay:0.6s;
}
.center span:nth-child(7) {
  animation-delay:0.7s;
}
.center span:nth-child(8) {
  animation-delay:0.8s;
}
.center span:nth-child(9) {
  animation-delay:0.9s;
}
.center span:nth-child(10) {
  animation-delay:1.0s;
}
.center span:nth-child(11) {
  animation-delay:1.1s;
}
.center span:nth-child(12) {
  animation-delay:1.2s;
}
@keyframes animate {
  from{font-size:1em}
  to{font-size:4em}
}
</style>
<body>
<div class="center">
  <span>C</span>
  <span>O</span>
  <span>M</span>
  <span>M</span>
  <span>I</span>
  <span>N</span>
  <span>G</span>
  <span>-</span>
  <span>S</span>
  <span>O</span>
  <span>O</span>
  <span>N</span>
</div>
